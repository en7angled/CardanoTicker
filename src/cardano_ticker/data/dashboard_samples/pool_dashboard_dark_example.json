{
    "canvas_size": [640, 400],
    "background_color": "blue",
    "dashboard": [
        {
            "type": "text",
            "size": [0.75, 0.1],
            "position": [0, 0],
            "data": {
                    "text": "@pool_name",
                    "auto_ajust_font_size": true
                    },
            "text_color": "blue",
            "background_color": "black"
        },
        {
            "type": "date_text",
            "size": [0.25, 0.1],
            "position": [0.75, 0],
            "data": {
                "font_size": 10
            },
            "background_color": "black",
            "text_color": "blue"
        },

        {
            "type": "pool_info_table",
            "size": [1, 0.19],
            "position": [0, 0.1],
            "data": {
                "pool_id": "@pool_id",
                "font_size": 11,
                "header_orientation": "columns"
            },
            "background_color": "blue",
            "text_color": "white",
            "header_color": "black"
        },
        {
            "type": "pool_stake_bar_chart",
            "position": [0.005, 0.3],
            "size": [0.5, 0.4],
            "data": {
                "pool_id": "@pool_id",
                "font_size": 8
            },
            "background_color": "blue"
        },
        {
            "type": "pool_history_chart",
            "position": [0.505, 0.3],
            "size": [0.5, 0.45],
            "data": {
                "pool_id": "@pool_id",
                "font_size": 8
            },
            "background_color": "blue",
            "text_color": "white"

        },
        {
            "type": "ada_price",
            "data": {
                "currency": ["usd", "btc", "eth"]
            },
            "position": [0.3, 0.75],
            "size": [0.4, 0.2],
            "text_color": "white",
            "background_color": "blue"
        }

    ]
}
